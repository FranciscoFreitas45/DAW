extends layout

block content
  .w3-container
    .w3-card-4
      header.w3-container.w3-teal.w3-margin-top
        h3 CLAV
    .w3-card-4
      header.w3-container.w3-teal.w3-margin-top
        h4 Lista de Entidades
      table.w3-table-all
        tr
          th Identificador
          th Sigla
          th Designação
          th Estado
        each ent in lista
                tr
                    td
                      a(href="/" + ent.id)=ent.id
                    td= ent.sigla
                    td= ent.designacao
                    td= ent.estado
        

